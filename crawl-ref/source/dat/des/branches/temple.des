##############################################################################
# temple.des: Entries for the Ecumenical Temple, and Temples themselves
#
# You can create temples with variable numbers of altars by specifying a
# "temple_variable" tag and one or more "temple_altars_#" tags with the
# number of altars you wish to place. Use get_altar_count() below to get
# the number of altars you need to place; see corrupted_temple for an
# example.
##############################################################################

{{
function get_altar_count(e, default)
  local params = dgn.map_parameters()
  local count = "";
  if params ~= nil then
    local tag = unpack(params)
    if tag ~= nil then
      -- temple_altars_ is 14 characters
      count = string.sub(tag, 15)
    end
  end
  if count == nil or count == "" then
     count = default
  end
  return count
end
}}

